{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "svelte": "rollup -c -w",
    "python": "python app.py",
    "start": "sirv public --no-clear",
    "dev": "concurrently -k -n \"SVELTE,FLASK\" -c \"cyan,magenta\" \"npm run svelte\" \"npm run python\""
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "postcss-import": "^16.1.1",
    "rollup": "^3.15.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-svelte": "^7.2.3",
    "svelte": "^4.0.0"
  },
  "dependencies": {
    "sirv-cli": "^2.0.0",
    "axios": "^1.6.0",
    "bootstrap": "^5.3.3",
    "font-awesome": "^4.7.0",
    "simplemde": "^1.11.2"
  }
}
